<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>ServerletDemo</servlet-name>
        <servlet-class>Serverlet.ServerletDemo</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServerletDemo</servlet-name>
        <url-pattern>/getInfo.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RaceCharaterSerlet</servlet-name>
        <servlet-class>Serverlet.RaceCharaterSerlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RaceCharaterSerlet</servlet-name>
            <url-pattern>/char.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RespByteServerlet</servlet-name>
        <servlet-class>Serverlet.RespByteServerlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RespByteServerlet</servlet-name>
        <url-pattern>/byte.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReDirectURL</servlet-name>
        <servlet-class>Serverlet.ReDirectURL</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReDirectURL</servlet-name>
        <url-pattern>/redirct.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>FileDownLoadServlet</servlet-name>
        <servlet-class>Serverlet.FileDownLoad</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileDownLoadServlet</servlet-name>
        <url-pattern>/down.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetBaseInfoServlet</servlet-name>
        <servlet-class>Serverlet.GetBaseInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetBaseInfoServlet</servlet-name>
        <url-pattern>/getBaseInfo.do</url-pattern>
    </servlet-mapping>
</web-app>